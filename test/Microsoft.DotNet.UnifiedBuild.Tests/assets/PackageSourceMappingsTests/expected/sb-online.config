<configuration>
  <packageSources>
    <clear />
    <add key="source-built-runtime" value="%runtime%" />
    <add key="source-built-arcade" value="%arcade%" />
    <add key="reference-packages" value="%reference-packages%" />
    <add key="previously-source-built" value="%previously-source-built%" />
    <add key="prebuilt" value="%prebuilt%" />
    <add key="dotnet-eng" value="https://pkgs.dev.azure.com/dnceng/public/_packaging/dotnet-eng/nuget/v3/index.json" />
    <add key="dotnet-tools" value="https://pkgs.dev.azure.com/dnceng/public/_packaging/dotnet-tools/nuget/v3/index.json" />
    <add key="dotnet-public" value="https://pkgs.dev.azure.com/dnceng/public/_packaging/dotnet-public/nuget/v3/index.json" />
  </packageSources>
  <disabledPackageSources>
    <clear />
  </disabledPackageSources>
  <packageSourceMapping>
    <clear />
    <packageSource key="source-built-runtime">
      <package pattern="runtime.package.one" />
      <package pattern="runtime.package.two" />
    </packageSource>
    <packageSource key="source-built-arcade">
      <package pattern="arcade.package.one" />
      <package pattern="arcade.package.two" />
    </packageSource>
    <packageSource key="reference-packages">
      <package pattern="reference.package.one" />
      <package pattern="reference.package.two" />
    </packageSource>
    <packageSource key="previously-source-built">
      <package pattern="psb.package.one" />
      <package pattern="psb.package.two" />
    </packageSource>
    <packageSource key="prebuilt">
      <package pattern="prebuilt.package" />
    </packageSource>
    <packageSource key="dotnet-eng">
      <package pattern="runtime.package.one" />
      <package pattern="runtime.package.two" />
      <package pattern="arcade.package.one" />
      <package pattern="arcade.package.two" />
      <package pattern="reference.package.one" />
      <package pattern="reference.package.two" />
      <package pattern="psb.package.one" />
      <package pattern="psb.package.two" />
      <package pattern="prebuilt.package" />
      <package pattern="*" />
    </packageSource>
    <packageSource key="dotnet-tools">
      <package pattern="runtime.package.one" />
      <package pattern="runtime.package.two" />
      <package pattern="arcade.package.one" />
      <package pattern="arcade.package.two" />
      <package pattern="reference.package.one" />
      <package pattern="reference.package.two" />
      <package pattern="psb.package.one" />
      <package pattern="psb.package.two" />
      <package pattern="prebuilt.package" />
      <package pattern="*" />
    </packageSource>
    <packageSource key="dotnet-public">
      <package pattern="runtime.package.one" />
      <package pattern="runtime.package.two" />
      <package pattern="arcade.package.one" />
      <package pattern="arcade.package.two" />
      <package pattern="reference.package.one" />
      <package pattern="reference.package.two" />
      <package pattern="psb.package.one" />
      <package pattern="psb.package.two" />
      <package pattern="prebuilt.package" />
      <package pattern="*" />
    </packageSource>
  </packageSourceMapping>
</configuration>